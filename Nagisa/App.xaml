<Application
  x:Class="Nagisa.App"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:local="using:Nagisa">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ms-appx:///CustomContentDialogStyle.xaml" />
        <ResourceDictionary Source="ms-appx:///CustomDimButtonStyle.xaml" />
        <ResourceDictionary Source="ms-appx:///CustomAutoSuggestBoxTextBoxStyle.xaml" />
      </ResourceDictionary.MergedDictionaries>

      <local:CustomConverter x:Name="CustomConverter" />

    </ResourceDictionary>
  </Application.Resources>
</Application>
